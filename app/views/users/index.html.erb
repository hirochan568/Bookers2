<h3>User index</h3>


<h2>Users</h2>
  <table class='table'>
    <% @users.each do |user| %>
    <tr>
    <td><%= attachment_image_tag user, :profile_image, :size, 20, 20, fallback: "no_image.jpg",size:'20x20',format: 'no_image.jpg' %></td>
    <th><%= user.name %><th>
    <th><%= link_to"Show",user_path(user.id) %></th>
    </tr>
    <% end %>
  </table>
